package java3;
import java.util.*;
class Demo{
		String name;
		int age;

		void display() {
			System.out.println("Name:"+name);
			System.out.println("Age:"+age);
		}	
	}
public class players{
public static void main(String[]args){
	Demo obj1=new Demo();
	obj1.name="gayathri";
	obj1.age=20;
	Demo obj2=new Demo();
	obj2.name="janani";
	obj2.age=16;
		obj1.display();
		obj2.display();
}
}
o/p:
Name:gayathri
Age:20
Name:janani
Age:16
		
		
		
		
	
		
	
	
